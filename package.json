{
  "name": "grendel-corporate-frontend",
  "private": true,
  "description": "Brochureware website for Grendel Consulting Limited",
  "version": "0.2.0",
  "author": "James Ramirez <hello@grendel-consulting.com>",
  "dependencies": {
    "@paralleldrive/feature-toggles": "^1.0.4",
    "@paralleldrive/react-feature-toggles": "^2.1.0",
    "@reach/router": "^1.2.1",
    "auth0-js": "^9.10.1",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.3.23",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sharp": "^2.0.31",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-sharp": "^2.1.13",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6",
    "styled-system": "^4.1.0",
    "styled-tachyons": "^1.0.7",
    "styled-theming": "^2.2.0",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "1.16.4",
    "standard-version": "^5.0.2"
  },
  "keywords": [
    "Grendel"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --single-quote --write src/**/*.{js,jsx,json,css,md}",
    "release": "standard-version",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grendel-consulting/corporate-frontend"
  },
  "bugs": {
    "url": "https://github.com/grendel-consulting/corporate-frontend/issues"
  }
}
