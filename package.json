{
  "name": "grendel-corporate-frontend",
  "private": true,
  "description": "Brochureware website for Grendel Consulting Limited",
  "version": "0.2.10",
  "author": "James Ramirez <hello@grendel-consulting.com>",
  "dependencies": {
    "@paralleldrive/feature-toggles": "1.0.4",
    "@paralleldrive/react-feature-toggles": "2.1.1",
    "@reach/router": "1.2.1",
    "auth0-js": "9.11.3",
    "gatsby": "2.17.7",
    "gatsby-image": "2.2.30",
    "gatsby-plugin-flow": "1.1.12",
    "gatsby-plugin-manifest": "2.2.26",
    "gatsby-plugin-offline": "2.2.10",
    "gatsby-plugin-react-helmet": "3.1.13",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.2.36",
    "gatsby-plugin-styled-components": "3.1.11",
    "gatsby-plugin-typography": "2.3.15",
    "gatsby-source-filesystem": "2.1.35",
    "gatsby-transformer-sharp": "2.3.2",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "styled-components": "4.4.0",
    "styled-normalize": "8.0.6",
    "styled-system": "4.2.3",
    "styled-tachyons": "1.0.8",
    "styled-theming": "2.2.0",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.7.0",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-preset-gatsby": "0.2.20",
    "eslint": "6.6.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "eslint-plugin-sonarjs": "0.4.0",
    "flow-bin": "0.111.0",
    "husky": "3.0.9",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "lint-staged": "9.4.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.11.0",
    "snyk": "1.241.0",
    "standard-version": "7.0.0",
    "stylelint": "11.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0"
  },
  "resolutions": {
    "lodash": "^4.17.11",
    "mem": "^4.0.0"
  },
  "keywords": [
    "Grendel"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --single-quote --write src/**/*.{js,jsx,json,css,md}",
    "release": "standard-version",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "pretest": "yarn run lint:css && yarn run lint",
    "test": "jest --passWithNoTests && snyk test",
    "lint": "eslint . --cache --fix",
    "lint:css": "stylelint '**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{css}": [
      "stylelint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grendel-consulting/corporate-frontend"
  },
  "bugs": {
    "url": "https://github.com/grendel-consulting/corporate-frontend/issues"
  }
}
