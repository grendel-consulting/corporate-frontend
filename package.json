    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-styled-components": "3.1.19",
    "gatsby-plugin-typography": "2.3.22",
    "gatsby-source-filesystem": "2.1.48",
    "gatsby-transformer-sharp": "2.3.14",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "styled-components": "5.0.1",
    "styled-normalize": "8.0.7",
    "styled-system": "5.1.4",
    "styled-tachyons": "1.0.8",
    "styled-theming": "2.2.0",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@storybook/addon-actions": "5.3.12",
    "@storybook/addon-links": "5.3.12",
    "@storybook/addons": "5.3.12",
    "@storybook/react": "5.3.12",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.7",
    "babel-preset-gatsby": "0.2.28",
    "cypress": "3.8.3",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-no-secrets": "0.3.4",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "flow-bin": "0.118.0",
    "husky": "4.2.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "snyk": "1.290.1",
    "standard-version": "7.1.0",
    "start-server-and-test": "1.10.8",
    "stylelint": "13.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.9.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint-plugin-flowtype": "^3.0.0 || ^4.0.0",
    "eslint-plugin-react-hooks": "^1.0.0 || ^2.0.0",
    "typescript": "2.8.0 || >= 3.2.0-dev || >= 3.3.0-dev || >= 3.4.0-dev || >= 3.5.0-dev || >= 3.6.0-dev || >= 3.6.0-beta || >= 3.7.0-dev || >= 3.7.0-beta"
  },
  "resolutions": {
    "dot-prop": "^5.1.1",
    "elliptic": "^6.5.2",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.13",
    "mem": "^4.0.0",
    "mixin-deep": "^1.3.2",
    "set-value": "^2.0.1",
    "url-parse": "^1.4.5"
  },
  "keywords": [
    "Grendel"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-storybook": "build-storybook",
    "cy:open": "cypress open",
    "develop": "gatsby develop",
    "format": "prettier --single-quote --write src/**/*.{js,jsx,json,css,md}",
    "release": "standard-version",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "pretest": "yarn run lint:css && yarn run lint",
    "test": "jest --passWithNoTests && snyk test",
    "lint": "eslint . --cache --fix",
    "lint:css": "stylelint '**/*.js'",
    "storybook": "start-storybook -p 6006",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{css}": [
      "stylelint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grendel-consulting/corporate-frontend"
  },
  "bugs": {
    "url": "https://github.com/grendel-consulting/corporate-frontend/issues"
  }
}
