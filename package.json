{
  "name": "grendel-corporate-frontend",
  "private": true,
  "description": "Brochureware website for Grendel Consulting Limited",
  "version": "0.2.6",
  "author": "James Ramirez <hello@grendel-consulting.com>",
  "dependencies": {
    "@paralleldrive/feature-toggles": "1.0.4",
    "@paralleldrive/react-feature-toggles": "2.1.1",
    "@reach/router": "1.2.1",
    "auth0-js": "9.10.2",
    "gatsby": "2.3.25",
    "gatsby-image": "2.0.39",
    "gatsby-plugin-flow": "1.0.4",
    "gatsby-plugin-manifest": "2.0.29",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-robots-txt": "1.4.0",
    "gatsby-plugin-sharp": "2.0.35",
    "gatsby-plugin-styled-components": "3.0.7",
    "gatsby-plugin-typography": "2.2.13",
    "gatsby-source-filesystem": "2.0.29",
    "gatsby-transformer-sharp": "2.1.18",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-typography": "0.16.19",
    "styled-components": "4.2.0",
    "styled-normalize": "8.0.6",
    "styled-system": "4.1.0",
    "styled-tachyons": "1.0.7",
    "styled-theming": "2.2.0",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "24.7.1",
    "babel-plugin-styled-components": "1.10.0",
    "babel-preset-gatsby": "0.1.11",
    "eslint": "5.16.0",
    "eslint-config-react-app": "3.0.8",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "flow-bin": "0.97.0",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "react-test-renderer": "16.8.6",
    "snyk": "1.151.2",
    "standard-version": "5.0.2",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.6.0"
  },
  "resolutions": {
    "conventional-changelog": "^1.0.0",
    "lodash": "^4.17.11",
    "mem": "^4.0.0"
  },
  "keywords": [
    "Grendel"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --single-quote --write src/**/*.{js,jsx,json,css,md}",
    "release": "standard-version",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "pretest": "yarn run lint:css && yarn run lint",
    "test": "jest --passWithNoTests && snyk test",
    "lint": "eslint . --cache --fix",
    "lint:css": "stylelint '**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{css}": [
      "stylelint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grendel-consulting/corporate-frontend"
  },
  "bugs": {
    "url": "https://github.com/grendel-consulting/corporate-frontend/issues"
  }
}
