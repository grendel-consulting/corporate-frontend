  "scripts": {
    "build": "gatsby build",
    "build-storybook": "build-storybook",
    "cy:open": "cypress open",
    "develop": "gatsby develop",
    "format": "prettier --single-quote --write src/**/*.{js,jsx,json,css,md}",
    "release": "standard-version",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "pretest": "yarn run lint:css && yarn run lint",
    "test": "jest --passWithNoTests && snyk test",
    "lint": "eslint . --cache --fix",
    "lint:css": "stylelint '**/*.js'",
    "storybook": "start-storybook -p 6006",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{css}": [
      "stylelint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grendel-consulting/corporate-frontend"
  },
  "bugs": {
    "url": "https://github.com/grendel-consulting/corporate-frontend/issues"
  },
  "snyk": true
}
